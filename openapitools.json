{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.3.0",
    "generators": {
      "datakatalog-java": {
        "inputSpec": "https://nvdbapiles.atlas.vegvesen.no/api-docs/datakatalog",
        "output": "java/datakatalog",
        "generatorName": "java",
        "library": "webclient",
        "apiPackage": "no.vegvesen.nvdb.datakatalog.api",
        "modelPackage": "no.vegvesen.nvdb.datakatalog.model",
        "globalProperty": "models,apis,modelDocs=false,modelTests=false,apiTests=false",
        "additionalProperties": {
          "hideGenerationTimestamp": true
        }
      },
      "datakatalog-kotlin": {
        "inputSpec": "https://nvdbapiles.atlas.vegvesen.no/api-docs/datakatalog",
        "output": "kotlin/datakatalog",
        "generatorName": "kotlin",
        "library": "multiplatform",
        "apiPackage": "no.vegvesen.nvdb.datakatalog.api",
        "modelPackage": "no.vegvesen.nvdb.datakatalog.model",
        "globalProperty": "models,apis,modelDocs=false,modelTests=false,apiTests=false",
        "additionalProperties": {
          "dateLibrary": "kotlinx-datetime",
          "sourceFolder": "src/main/kotlin"
        }
      },
      "vegnett-kotlin": {
        "inputSpec": "https://nvdbapiles.atlas.vegvesen.no/api-docs/vegnett",
        "output": "kotlin/vegnett",
        "generatorName": "kotlin",
        "library": "multiplatform",
        "apiPackage": "no.vegvesen.nvdb.vegnett.api",
        "modelPackage": "no.vegvesen.nvdb.vegnett.model",
        "globalProperty": "models,apis,modelDocs=false,modelTests=false,apiTests=false",
        "additionalProperties": {
          "dateLibrary": "kotlinx-datetime",
          "sourceFolder": "src/main/kotlin"
        }
      },
      "datakatalog-ts": {
        "inputSpec": "https://nvdbapiles.atlas.vegvesen.no/api-docs/datakatalog",
        "output": "typescript/datakatalog",
        "generatorName": "typescript-fetch"
      },
      "vegobjekter-kotlin": {
        "inputSpec": "https://nvdbapiles.atlas.vegvesen.no/api-docs/vegobjekter",
        "output": "kotlin/vegobjekter",
        "generatorName": "kotlin",
        "library": "multiplatform",
        "apiPackage": "no.vegvesen.nvdb.vegobjekter.api",
        "modelPackage": "no.vegvesen.nvdb.vegobjekter.model",
        "globalProperty": "models,apis,modelDocs=false,modelTests=false,apiTests=false",
        "additionalProperties": {
          "dateLibrary": "kotlinx-datetime",
          "sourceFolder": "src/main/kotlin"
        }
      }
    }
  }
}
